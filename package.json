{
	"name": "react-router-with-container",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "react-router build",
		"typegen": "pnpm run --stream --color \"/^typegen:/\"",
		"typegen:cloudflare": "wrangler types --env-interface CloudflareBindings",
		"typegen:react-router": "react-router typegen",
		"deploy": "pnpm run build && wrangler deploy",
		"dev": "react-router dev",
		"postinstall": "pnpm run typegen",
		"preview": "pnpm run build && vite preview",
		"typecheck": "pnpm run typegen && tsc -b"
	},
	"dependencies": {
		"hono": "4.10.7",
		"isbot": "^5.1.32",
		"react": "^19.2.1",
		"react-dom": "^19.2.1",
		"react-router": "^7.10.0"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "^1.16.1",
		"@react-router/dev": "^7.10.0",
		"@tailwindcss/vite": "^4.1.17",
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3",
		"vite": "^7.2.6",
		"wrangler": "^4.52.1"
	}
}
