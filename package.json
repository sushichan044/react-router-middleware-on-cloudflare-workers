{
	"name": "react-router-with-container",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "react-router build",
		"typegen": "pnpm run --stream --color \"/^typegen:/\"",
		"typegen:cloudflare": "wrangler types --env-interface CloudflareBindings",
		"typegen:react-router": "react-router typegen",
		"deploy": "pnpm run build && wrangler deploy",
		"dev": "react-router dev",
		"postinstall": "pnpm run typegen",
		"preview": "pnpm run build && vite preview",
		"typecheck": "pnpm run typegen && tsc -b"
	},
	"dependencies": {
		"hono": "4.9.7",
		"isbot": "^5.1.30",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-router": "^7.9.1"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "^1.13.0",
		"@react-router/dev": "^7.9.1",
		"@tailwindcss/vite": "^4.1.13",
		"@types/node": "^24.3.3",
		"@types/react": "^19.1.13",
		"@types/react-dom": "^19.1.9",
		"tailwindcss": "^4.1.13",
		"typescript": "^5.9.2",
		"vite": "^7.1.5",
		"wrangler": "^4.36.0"
	}
}
